{
  "name": "Boilerplate-client",
  "version": "0.1.0",
  "private": true,
  "description": "Boilerplate Client",
  "scripts": {
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "start": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development nodemon index.js",
    "start:prod": "cross-env NODE_ENV=production node index.js",
    "bs": "rimraf dist && npm run build && npm run build:server && npm run start:prod",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:server": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=production webpack --config webpack.config.server.js",
    "lint": "eslint client server",
    "storybook": "./node_modules/.bin/start-storybook -p 9001",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "babel-core": "^6.20.0",
    "body-parser": "^1.15.2",
    "browser-cookies": "^1.0.8",
    "classnames": "^2.2.5",
