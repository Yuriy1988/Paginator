    "json-loader": "^0.5.4",
    "mock-css-modules": "^1.0.0",
    "nock": "^9.0.2",
    "nodemon": "^1.11.0",
    "null-loader": "^0.1.1",
    "postcss-cssnext": "^2.9.0",
    "postcss-focus": "^1.0.0",
    "postcss-loader": "^1.2.1",
    "postcss-reporter": "^3.0.0",
    "postcss-scss": "^0.4.0",
    "pre-commit": "^1.2.2",
    "precss": "^1.4.0",
