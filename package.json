    "cookie-parser": "^1.4.3",
    "cross-env": "^3.1.3",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "material-ui": "^0.16.7",
    "mobile-detect": "^1.3.5",
    "moment": "^2.17.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-dropzone": "^3.7.3",
    "react-easy-transition": "0.0.5",
    "react-helmet": "^4.0.0",
    "react-highcharts": "^11.5.0",
    "react-icons": "^2.2.3",
    "react-motion": "^0.4.7",
    "react-redux": "^5.0.2",
    "react-redux-loading-bar": "^2.6.5",
    "react-router": "3.0.2",
    "react-router-redux": "^4.0.7",
    "react-router-scroll": "^0.4.1",
    "react-router-transition": "0.0.6",
    "react-select": "^1.0.0-rc.2",
    "react-sticky": "^5.0.5",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-async-connect": "^1.0.0-rc4",
    "redux-connect": "^5.0.0",
    "redux-form": "^6.5.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.2.0",
    "styled-components": "^1.4.2"
  },
